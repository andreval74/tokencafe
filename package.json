{
  "name": "tokencafe",
  "version": "1.0.0",
  "description": "Ecossistema unificado de widgets de token - Inspirado na cultura cafeeira brasileira",
  "main": "api/server.js",
  "scripts": {
    "dev": "node api/server.js",
    "build": "node -e \"console.log('Build não necessário para páginas estáticas; pulando.')\"",
    "test": "node -e \"console.log('Sem testes oficiais no projeto');\"",
    "test:watch": "node -e \"console.log('Sem testes oficiais no projeto');\"",
    "start": "node api/server.js",
    "lint": "eslint js/**/*.js api/**/*.js",
    "lint:fix": "eslint js/**/*.js api/**/*.js --fix",
    "format": "prettier --write \"**/*.{js,css,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,css,json,md}\"",
    "docs": "jsdoc -c jsdoc.conf.json",
    "setup:dev": "npm install && npm run build",
    "deploy": "npm run test && node -e \"console.log('Pronto para deploy: servir com node api/server.js ou infraestrutura estática.')\""
  },
  "keywords": [
    "tokencafe",
    "blockchain",
    "widgets",
    "tokens",
    "web3",
    "cafe",
    "brasil",
    "tokencafe",
    "widget",
    "saas"
  ],
  "author": "TokenCafe Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/andreval74/tokencafe.git"
  },
  "homepage": "https://tokencafe.app",
  "dependencies": {
    "@web3-onboard/coinbase": "^2.4.2",
    "@web3-onboard/core": "^2.24.1",
    "@web3-onboard/injected-wallets": "^2.11.3",
    "@web3-onboard/ledger": "^2.7.1",
    "@web3-onboard/metamask": "^2.2.1",
    "@web3-onboard/trezor": "^2.4.7",
    "@web3-onboard/trust": "^2.1.2",
    "@web3-onboard/walletconnect": "^2.6.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.6.7",
    "express": "^4.21.2",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.4.0",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "redis": "^4.6.7",
    "socket.io": "^4.7.2",
    "solc": "^0.8.33",
    "web3": "^4.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "eslint": "^8.45.0",
    "jest": "^29.6.0",
    "jsdoc": "^4.0.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.6.2",
    "supertest": "^6.3.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
